{
    "parser": "babel-eslint",
    "extends": [
        "airbnb",
        "plugin:jest/recommended"
    ],
    "rules": {
        "react/jsx-filename-extension": "ignore",
        "jsx-a11y/anchor-is-valid": "ignore",
        "no-underscore-dangle": 0
    },
    "env": {
        "browser": true
    },
    "plugins": ["jest"]
}